<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/mattdrewitt/unraid-templates/master/mattdrewitt/music-queue.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>MediaServer:Music</Category>
  <Name>music-queue</Name>
  <Description></Description>
  <Overview></Overview>
  <Support></Support>
  <Registry></Registry>
  <GitHub>https://github.com/mattdrewitt/music-queue</GitHub>
  <Repository>music_queue</Repository>
  <Project></Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>3001</HostPort>
        <ContainerPort>3001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>MUSIC_QUEUE_SPOTIFY_ID</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_SPOTIFY_SECRET</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_YOUTUBE_KEY</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_REDIS_URL</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_DB_HOST</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_DB_USERNAME</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_DB_PASSWORD</Name>
      <Value></Value>
    </Variable>
     <Variable>
      <Name>MUSIC_QUEUE_GMAIL_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>MUSIC_QUEUE_ELASTICSEARCH_HOST</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>SECRET_KEY_BASE</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>RAILS_ENV</Name>
      <Value>production</Value>
    </Variable>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>0</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/music-queue/log</HostDir>
      <ContainerDir>/usr/src/app/log</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:3000]/</WebUI>
  <Icon>https://raw.githubusercontent.com/mattdrewitt/unraid-templates/master/mattdrewitt/images/jukebox.png</Icon>
  <ExtraParams></ExtraParams>
</Containers>
